<div class="container">
 <div class="row">
  <%= render "info" %>

  <div class="col md-8 offset-md-1">
   <h2>Users</h2>
    <table class="table table-hover table-inverse">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th colspan="3"></th>
        </tr>
      </thead>
      <tbody>
       <tr>
        <% @users.each do |user| %>
         <td><%= image_tag @user.get_profile_image %></td>
         <td><%= user.name %></td>
         <td><%= link_to 'Show', user_path(user.id) %></td>
        <% end %>
       </tr>
      </tbody>
    </table>
</div>

